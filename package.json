{
  "name": "unstack",
  "version": "0.2.4",
  "description": "Print your favorite framework's logo and details into the cli",
  "repository": "shba007/framework-logo",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "keywords": [
    "art",
    "ascii art",
    "framework",
    "logo",
    "angular",
    "react",
    "vuejs",
    "svelte",
    "preact",
    "solidjs",
    "remix",
    "qwik",
    "lit",
    "tinijs",
    "alpinejs",
    "stencil",
    "mithril",
    "astro",
    "vuepress",
    "vitepress",
    "docus",
    "meteor",
    "express",
    "fastify",
    "koa",
    "feathers",
    "nestjs",
    "nitro",
    "analog",
    "next",
    "gatsby",
    "nuxt",
    "gridsome",
    "svelte-kit",
    "ember",
    "fresh",
    "redwood",
    "hydrogen"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "unstack": "dist/index.mjs"
  },
  "files": [
    "assets",
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "volta": {
    "node": "20.17.0"
  },
  "engines": {
    "node": "^20.17.0"
  },
  "scripts": {
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "eslint . --fix && prettier -w .",
    "build": "unbuild",
    "prepack": "npm build",
    "play": "jiti playground",
    "release": "npm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "npm lint && npm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "unstack": "jiti ./src/cli"
  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1",
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "date-fns": "^4.1.0",
    "ofetch": "^1.4.0",
    "ora": "^8.1.0",
    "unascii": "^0.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/node": "^22.7.4",
    "@vitest/coverage-v8": "^2.1.1",
    "changelogen": "^0.5.7",
    "eslint": "^8.57.1",
    "eslint-config-unjs": "^0.4.1",
    "jiti": "^2.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "unbuild": "^3.0.0-rc.8",
    "unenv": "^1.10.0",
    "vitest": "^2.1.1"
  }
}
