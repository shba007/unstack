{
  "name": "@shba007/unstack",
  "version": "1.0.0",
  "description": "Print your favorite framework info into cli",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shba007/unstack.git"
  },
  "keywords": [
    "art",
    "ascii art",
    "framework",
    "logo",
    "angular",
    "react",
    "vuejs",
    "svelte",
    "preact",
    "solidjs",
    "remix",
    "qwik",
    "lit",
    "tinijs",
    "alpinejs",
    "stencil",
    "mithril",
    "astro",
    "vuepress",
    "vitepress",
    "docus",
    "meteor",
    "express",
    "fastify",
    "koa",
    "feathers",
    "nestjs",
    "nitro",
    "analog",
    "next",
    "gatsby",
    "nuxt",
    "gridsome",
    "svelte-kit",
    "ember",
    "fresh",
    "redwood",
    "hydrogen"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "unstack": "./bin/unstack.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.2.21",
  "scripts": {
    "prepare": "husky || true",
    "dev": "dotenvx run -- vitest dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test": "bun lint && bun test:types && dotenvx run -- vitest run --coverage",
    "play": "dotenvx run -- jiti examples",
    "build": "unbuild",
    "prepack": "bun run build",
    "clean": "rm -rf coverage dist",
    "unstack": "bun bin/unstack.mjs"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.4",
    "@shba007/unascii": "^1.0.2",
    "chalk": "^5.6.0",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "ofetch": "^1.4.1",
    "ora": "^8.2.0",
    "std-env": "^3.9.0",
    "string-width": "^8.1.0",
    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@dotenvx/dotenvx": "^1.49.0",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-unjs": "^0.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "unbuild": "3.5.0",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "canvas",
    "esbuild"
  ]
}
