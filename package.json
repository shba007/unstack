{
  "name": "@shba007/unstack",
  "version": "0.3.7",
  "description": "Print your favorite framework's logo and details into the cli",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shba007/unstack.git"
  },
  "keywords": [
    "art",
    "ascii art",
    "framework",
    "logo",
    "angular",
    "react",
    "vuejs",
    "svelte",
    "preact",
    "solidjs",
    "remix",
    "qwik",
    "lit",
    "tinijs",
    "alpinejs",
    "stencil",
    "mithril",
    "astro",
    "vuepress",
    "vitepress",
    "docus",
    "meteor",
    "express",
    "fastify",
    "koa",
    "feathers",
    "nestjs",
    "nitro",
    "analog",
    "next",
    "gatsby",
    "nuxt",
    "gridsome",
    "svelte-kit",
    "ember",
    "fresh",
    "redwood",
    "hydrogen"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "unstack": "./bin/unstack.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "volta": {
    "node": "22.14.0"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0"
  },
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "prepare": "husky || true",
    "dev": "vitest dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "play": "dotenvx run -- jiti playground",
    "build": "unbuild",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && pnpm publish && git push --follow-tags",
    "clean": "rm -rf coverage dist",
    "unstack": "cd bin && node unstack.mjs"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.3.3",
    "@shba007/unascii": "^0.4.0",
    "chalk": "^5.4.1",
    "citty": "^0.1.6",
    "consola": "^3.4.0",
    "date-fns": "^4.1.0",
    "ofetch": "^1.4.1",
    "ora": "^8.2.0",
    "std-env": "^3.8.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@dotenvx/dotenvx": "^1.38.5",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "eslint-config-unjs": "^0.4.2",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "unbuild": "3.5.0",
    "vitest": "^3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild"
    ]
  }
}